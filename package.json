{
  "name": "certifymyrepo-vjs",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build:preprod": "vue-cli-service build --mode preproduction",
    "build:prod": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "@sedoo/unidoo": "0.0.53",
    "apexcharts": "^3.29.0",
    "axios": "^0.21.1",
    "core-js": "^2.6.12",
    "es6-promise": "^4.2.8",
    "js-cookie": "^2.2.1",
    "jsonp": "^0.2.1",
    "moment": "^2.29.1",
    "qs": "^6.9.6",
    "vue": "^2.6.12",
    "vue-apexcharts": "^1.6.2",
    "vue-axios": "^2.1.5",
    "vue-custom-element": "^3.2.14",
    "vue-i18n": "^8.22.4",
    "vuetify": "2.4.3",
    "vuex": "^3.6.0",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.1.0",
    "eslint": "7.10.0",
    "eslint-plugin-vue": "7.0.1",
    "sass": "1.27.0",
    "sass-loader": "10.0.2",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "2.5.8",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0",
    "webpack": "^5.75.0",
    "webpack-cdn-upload-plugin": "^2.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "no-mixed-spaces-and-tabs": 0,
      "no-empty": 0,
      "no-unused-vars": 0,
      "no-useless-escape": 0,
      "no-control-regex": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
